{
  "name": "TapInGuide",
  "version": "3.0.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.0.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "pui-react-expander": "^7.5.4",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-color": "^2.12.0",
    "react-dom": "^15.4.2",
    "react-flip-move": "^2.9.14",
    "react-loader": "^2.4.2",
    "react-loader-advanced": "^1.4.1",
    "react-moment": "^0.1.3",
    "react-render-html": "^0.1.6",
    "react-router-dom": "^4.1.1",
    "reactcss": "^1.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/styles/styles.scss -o src/assets/styles/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/styles/styles.scss -o src/assets/styles/ --watch --recursive --output-style compressed",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
